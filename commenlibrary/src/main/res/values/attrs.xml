<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiStateView">
        <attr name="loadingView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="networkErrorView" format="reference" />
        <attr name="viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="networkError" value="2" />
            <enum name="empty" value="3" />
            <enum name="loading" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DrawableTextView">
        <attr name="drawableWidth" format="dimension"/>
        <attr name="drawableHight" format="dimension"/>
    </declare-styleable>


</resources>